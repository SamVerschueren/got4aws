{
	"name": "got4aws",
	"version": "1.2.0",
	"description": "Convenience wrapper for Got to interact with AWS v4 signed APIs",
	"license": "MIT",
	"repository": "SamVerschueren/got4aws",
	"author": {
		"name": "Sam Verschueren",
		"email": "sam.verschueren@gmail.com",
		"url": "https://github.com/SamVerschueren"
	},
	"main": "dist/index.js",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"pretest": "npm run compile -- -p tsconfig.spec.json",
		"test": "xo && nyc ava",
		"compile": "del-cli dist && tsc"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"got-plugin",
		"got",
		"aws",
		"aws4",
		"api",
		"sigv4",
		"request",
		"http",
		"https",
		"get",
		"url",
		"utility"
	],
	"peerDependencies": {
		"aws-sdk": "^2.700.0"
	},
	"dependencies": {
		"aws4": "^1.11.0",
		"got": "^11.8.0"
	},
	"devDependencies": {
		"@ava/typescript": "^1.1.1",
		"@sindresorhus/tsconfig": "^0.7.0",
		"@types/aws4": "^1.5.1",
		"@types/sinon": "^9.0.4",
		"ava": "^3.10.1",
		"aws-sdk": "^2.700.0",
		"del-cli": "^3.0.1",
		"nock": "^13.0.2",
		"nyc": "^15.1.0",
		"sinon": "^9.0.2",
		"typescript": "^3.9.5",
		"xo": "^0.32.0"
	},
	"ava": {
		"typescript": {
			"rewritePaths": {
				"source/": "dist/"
			}
		}
	},
	"nyc": {
		"exclude": [
			"source/test.ts"
		]
	}
}
